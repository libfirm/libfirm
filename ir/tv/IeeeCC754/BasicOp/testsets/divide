2/       <       -1       1i1   x   -1d1
2/       =0>     1      -1i1   x   -1d2
2/       <       1      -1i1   x   -1d1
2/       =0<     1       1i2   x   1d4
2/       >       1       1i2   x   1d3
2/       =       1       1i3   x   1d6
2/       0       1       1i3   x   1d6
2/       <       1       1i3   x   1d6
2/       >       1       1i3   x   1d5
2/       =       1       1i4   x   1d8
2/       0       1       1i4   x   1d8
2/       <       1       1i4   x   1d8
2/       >       1       1i4   x   1d7
! 1 / (1 - Nu-) --> 1 + (Q/2 u+) + tiny.
2/       =       1       1d2   x   1i1
2/       0       1       1d2   x   1i1
2/       <       1       1d2   x   1i1
2/       >       1       1d2   x   1i2
